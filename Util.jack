class Util {
	// utility
	static int _lastKey_key;
	function int lastKey() {
		var int c;
		let c = Keyboard.keyPressed();
		if (c=_lastKey_key) {
			return 0;
		} else {
			let _lastKey_key = c;
			return c;
		}
	}
	function int modulo(int a, int b) {
		return a - ( a / b * b );
	}
	function int xor(int a, int b) {
		return (~(a & b)) & (a | b);
	}
}
